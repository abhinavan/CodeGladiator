package dataSturcturePack;

public class ArrayOperationClass 
{
	public int[] deleteItem(int[] arr,int key)
	{
		int positionFind=0;
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]==key)
			{
				arr[i]=arr[i+1];
				break;
			}
		}
		
		return null;
	}
	public static void main(String[] args) 
	{
		int[] arr={1,2,3,4,5,6};
		int key=4;
		new ArrayOperationClass().deleteItem(arr, key);
		
	}

}
