public class HeightBubbleSortClass 
{
	static 
	{
		Scanner sc=new Scanner(System.in);
	}
	public static int[] sort(int[] arr)
	{
		for(int i=0;i<arr.length-1;i++)
		{
			if(arr[i]>=arr[i+1])
			{
				
			}
		}
		return null;
	}
	public static void main(String[] args) 
	{
		int[] arr={2,3,4,1,5};
		sort(arr);
		
	}
	
}
